{
    "name": "open-admin-ext/multi-language",
    "description": "Allow to multilanguage support for open-admin based on laravel-translatable, Credits for orginal implementation: Andrey Kravchenko",
    "type": "library",
    "keywords": [
        "open-admin",
        "extension"
    ],
    "homepage": "https://github.com/open-admin-org/multi-language",
    "license": "MIT",
    "authors": [
        {
            "name": "Sjors Broersen",
            "email": "sjors@bootmine.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "astrotomic/laravel-translatable": "^11.15",
        "open-admin-org/open-admin": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "OpenAdmin\\MultiLanguage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenAdmin\\MultiLanguage\\MultiLanguageServiceProvider"
            ]
        }
    }
}